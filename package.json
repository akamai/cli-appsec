{
  "name": "akamaicliappsec",
  "version": "1.0.0",
  "description": "A wrapping development kit to interface common tasks with akamai's Security {OPEN} API.",
  "repository": "https://github.com/akamai/cli-appsec",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=7.0.0"
  },
  "scripts": {
    "lint": "eslint src",
    "test": "mocha",
    "precommit": "lint-staged",
    "build": "pkg . --target node8-linux-x86,node8-linux-x64,node8-win-x86,node8-win-x64,node8-macos-x64 --output akamai-appsec-1.0.0"
  },
  "bin": {
    "appsec": "bin/akamai-appsec"
  },
  "files": [
    "bin/commands",
    "src",
    "templates"
  ],
  "dependencies": {
    "child-process-promise": "^2.2.1",
    "edgegrid": "^3.0.4",
    "pino": "^4.10.4",
    "string-format": "^0.5.0",
    "sywac": "^1.2.0",
    "untildify": "^3.0.2",
    "yarn": "^1.3.2"
  },
  "devDependencies": {
    "pkg": "^4.3.0-beta.5",
    "chai": "^4.1.2",
    "eslint": "^4.17.0",
    "fs": "0.0.1-security",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "mocha": "^5.0.0",
    "nock": "^9.1.6",
    "prettier": "^1.6.1",
    "sinon": "^4.2.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --print-width 100 --write",
      "git add"
    ]
  }
}
